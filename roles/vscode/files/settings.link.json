{
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "diffEditor.codeLens": true,
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingQuotes": "always",
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.codeActionsOnSave": {
    "addMissingImports": true,
    "source.fixAll": true
  },
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorStyle": "block",
  "editor.cursorSurroundingLines": 2,
  "editor.cursorSurroundingLinesStyle": "all",
  "editor.cursorWidth": 3,
  "editor.defaultFormatter": "vscode.typescript-language-features",
  "editor.dragAndDrop": false,
  "editor.emptySelectionClipboard": false,
  "editor.folding": false,
  "editor.fontFamily": "'Cascadia Code', 'Fira Code', 'Droid Sans Mono', 'monospace', monospace",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.padding.bottom": 20,
  "editor.padding.top": 20,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    80,
    110,
    120
  ],
  "editor.scrollbar.verticalScrollbarSize": 18,
  "editor.scrollbar.horizontalScrollbarSize": 18,
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.unusualLineTerminators": "auto",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "explorer.confirmDelete": false,
  "files.autoSave": "afterDelay",
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/__pycache__": true,
    "**/node_modules": true,
    "**/vendor": true,
    "dist": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/__pycache__/**": true,
    "**/vendor/**": true
  },
  "git.confirmSync": false,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "javascript.format.semicolons": "remove",
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.preferences.quoteStyle": "single",
  "javascript.suggest.completeFunctionCalls": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "php.validate.executablePath": "/usr/bin/php",
  "python.formatting.provider": "black",
  "redhat.telemetry.enabled": false,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.quoteStyle": "single",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.autoDetectColorScheme": true,
  "window.autoDetectHighContrast": false,
  "window.menuBarVisibility": "toggle",
  "window.restoreWindows": "none",
  "window.newWindowDimensions": "inherit",
  "workbench.colorTheme": "Cobalt2",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.sideBar.location": "right",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.value": 6,
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.wrapTabs": true,
  "workbench.enableExperiments": false,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false
}
