Host github.com
  HostName github.com
  AddKeysToAgent yes
  IdentityFile ~/.ssh/github.personal
  User git

Host gitlab.com
  HostName gitlab.com
  AddKeysToAgent yes
  IdentityFile ~/.ssh/gitlab.personal
  User git

Host gitlab.com-work
  HostName gitlab.com
  AddKeysToAgent yes
  IdentityFile ~/.ssh/gitlab.work
  User git

Host github.com-work
  HostName github.com
  AddKeysToAgent yes
  IdentityFile ~/.ssh/github.work
  User git
